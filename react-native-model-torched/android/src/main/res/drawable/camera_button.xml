<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <!-- Outer stroke -->
  <item android:width="@dimen/camera_button_size_normal" android:height="@dimen/camera_button_size_normal" android:id="@+id/camera_button_outer_circle">
    <shape android:shape="oval">
      <stroke android:width="@dimen/camera_button_border" android:color="@android:color/white" />
    </shape>
  </item>
  <!-- Smaller inner circle -->
  <item android:id="@+id/camera_button_inner_circle">
    <shape android:shape="oval">
      <stroke android:width="@dimen/camera_button_inner_border_normal" android:color="@android:color/transparent" />
      <solid android:color="@android:color/white" />
    </shape>
  </item>
</layer-list>
